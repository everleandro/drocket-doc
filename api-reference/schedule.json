[
    {
        "property": "lng",
        "description": "component language",
        "type": {
            "value": "es | en",
            "class": "string"
        },
        "default": {
            "value": "'en'",
            "class": "string"
        }
    },
    {
        "property": "color",
        "description": "Applies specified color to the component - supports only utility colors (for example <code class='boolean'>primary</code> or <code class='boolean'>secondary</code>)",
        "type": {
            "value": "string",
            "class": "string"
        },
        "default": {
            "value": "'primary'",
            "class": "string"
        }
    },
    {
        "property": "loading",
        "description": "Show a linear progress bar at the bottom of the header and change the status to read-only",
        "type": {
            "value": "boolean",
            "class": "string"
        },
        "default": {
            "value": "false",
            "class": "boolean"
        }
    },
    {
        "property": "schedule-column",
        "description": "maximum number of columns to display (only applies when <code class='string'>schedule-mode</code> is <code class='string'>schedule</code>)",
        "type": {
            "value": "number | string",
            "class": "string"
        },
        "default": {
            "value": "undefined",
            "class": "undefined"
        }
    },
    {
        "property": "row-height",
        "description": "row height in pixels",
        "type": {
            "value": "number | string",
            "class": "string"
        },
        "default": {
            "value": "97",
            "class": "string"
        }
    },
    {
        "property": "step",
        "description": "determines the time (seconds) that elapses between one row and the other",
        "type": {
            "value": "number",
            "class": "string"
        },
        "default": {
            "value": "60 * 60",
            "class": "string"
        }
    },
    {
        "property": "start",
        "description": "determines the time(seconds) from which the first row will be displayed",
        "type": {
            "value": "number",
            "class": "string"
        },
        "default": {
            "value": "0",
            "class": "string"
        }
    },
    {
        "property": "end",
        "description": "determines the last time(seconds) the schedule will display",
        "type": {
            "value": "number",
            "class": "string"
        },
        "default": {
            "value": " 60 * 60 * 24 ",
            "class": "string"
        }
    },
    {
        "property": "ScheduleSpace",
        "description": "selected space is used to display the events of that space (only applies when <code class='string'>schedule-mode</code> is <code class='string'>week</code> or <code class='string'>day</code>)",
        "type": {
            "value": "ScheduleSpace",
            "class": "string"
        },
        "default": {
            "value": "undefined",
            "class": "undefined"
        }
    },
    {
        "property": "modelValue",
        "description": "selected date is used to display the events of that date ",
        "type": {
            "value": "Date",
            "class": "string"
        },
        "default": {
            "value": "new Date()",
            "class": "boolean"
        }
    },
    {
        "property": "mode",
        "description": "determines the type of schedule to display",
        "type": {
            "value": "ScheduleMode",
            "class": "string"
        },
        "default": {
            "value": "ScheduleMode.day",
            "class": "boolean"
        }
    },
    {
        "property": "spaces",
        "description": "available spaces",
        "type": {
            "value": "ScheduleSpace[]",
            "class": "string"
        },
        "default": {
            "value": "[]",
            "class": "string"
        }
    },
    {
        "property": "events",
        "description": "scheduled events",
        "type": {
            "value": "ScheduleEvent[]",
            "class": "string"
        },
        "default": {
            "value": "[]",
            "class": "string"
        }
    },
    {
        "property": "sticky-top-header",
        "description": "value of the css <code class='string'>top</code> property for the component headers, changes the positioning of the headers to sticky automatically)",
        "type": {
            "value": "string | number",
            "class": "string"
        },
        "default": {
            "value": "0",
            "class": "string"
        }
    }
]